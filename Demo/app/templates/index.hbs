{{#link-to "new"}}Create New Record{{/link-to}}

<ul>
{{#each}}
	<li>
		{{#link-to "view" this}}{{firstName}} {{middleName}}{{/link-to}}
		&nbsp;&nbsp;({{#link-to "edit" this}}Edit{{/link-to}})
		&nbsp;&nbsp;(<a href="javascript:void(0)" {{action "efcDelete" this}}>Delete</a>)</li>
{{/each}}
</ul>


<div class="dl-table">
	<dl class="header">
		<dt>First Name</dt><dd>&nbsp;</dd>
		<dt>Middle Name</dt><dd>&nbsp;</dd>
	</dl>
	{{#each}}
	<dl>
		<dt>First Name</dt><dd>{{firstName}}</dd>
		<dt>Middle Name</dt><dd>{{middleName}}</dd>
	</dl>
	{{/each}}
</div>